{
  "expo": {
    "name": "Jotter",
    "slug": "jotter-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/jotter-circle2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/imgs/jotter-circle2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/jotter-squircle3.png",
        "backgroundColor": "#000000"
      },
      "package": "com.javacow.jotter"
    },
    "web": {
      "favicon": "./assets/imgs/jotter-circle2.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Regular2.ttf",
            "./assets/fonts/Inter-Italic2.ttf",
            "./assets/fonts/Inter-Bold2.ttf",
            "./assets/fonts/Inter-SemiBold2.ttf",
            "./assets/fonts/Inter-ExtraBold2.ttf",
            "./assets/fonts/Inter-BoldItalic2.ttf",
            "./assets/fonts/RobotoMono-Regular.ttf"
          ]
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "2a426c49-a633-40b6-b237-b2c537bd1e47"
      }
    }
  }
}
